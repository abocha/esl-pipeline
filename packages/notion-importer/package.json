{
  "name": "@esl-pipeline/notion-importer",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "description": "Import validated ESL assignments into Notion databases.",
  "keywords": [
    "esl",
    "notion",
    "import",
    "cli"
  ],
  "engines": {
    "node": ">=18.18"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "notion-importer": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "clean": "rimraf dist",
    "dev": "tsx watch bin/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@esl-pipeline/md-extractor": "workspace:*",
    "@esl-pipeline/md-validator": "workspace:*",
    "@notionhq/client": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  }
}
