{
  "name": "@esl-pipeline/notion-add-audio",
  "version": "1.2.3",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "description": "Attach generated audio blocks to Notion study-text toggles.",
  "keywords": [
    "esl",
    "notion",
    "audio"
  ],
  "engines": {
    "node": ">=24.10.0"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "notion-add-audio": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "node -e \"fs.rmSync('dist', {recursive:true, force:true})\"",
    "dev": "tsx watch bin/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@notionhq/client": "^5.4.0",
    "commander": "^14.0.2",
    "@esl-pipeline/contracts": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  }
}