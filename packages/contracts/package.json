{
  "name": "@esl-pipeline/contracts",
  "version": "1.0.1",
  "private": true,
  "description": "Shared TypeScript contracts for ESL pipeline services (job DTOs, events, enums).",
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    ".": {
