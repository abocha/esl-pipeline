{
  "name": "@esl-pipeline/storage-uploader",
  "version": "1.2.2",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "description": "Upload ESL assignment assets (audio) to S3-compatible storage.",
  "keywords": [
    "esl",
    "s3",
    "upload",
    "cli"
  ],
  "engines": {
    "node": ">=24.10.0"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "storage-uploader": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "node -e \"fs.rmSync('dist', {recursive:true, force:true})\"",
    "dev": "tsx watch bin/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.936.0",
    "@aws-sdk/s3-request-presigner": "^3.936.0",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  }
}