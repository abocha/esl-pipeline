  "private": true,
  "type": "module",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1"
  },
  "dependencies": {
    "@types/mdast": "^4.0.4",
    "remark-parse": "^11.0.0"
