{
  "name": "esl-pipeline",
  "version": "1.9.2",
  "private": true,
  "description": "ESL assignment pipeline tooling (validation, Notion import, TTS, S3 upload).",
  "keywords": [
    "esl",
    "notion",
    "tts",
    "cli",
    "pipeline"
  ],
  "homepage": "https://github.com/abocha/esl-pipeline",
  "bugs": {
    "url": "https://github.com/abocha/esl-pipeline/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abocha/esl-pipeline.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r --filter './packages/*' run build",
    "build:add-audio": "pnpm --filter @esl-pipeline/notion-add-audio build",
    "build:colorizer": "pnpm --filter @esl-pipeline/notion-colorizer build",
    "build:extractor": "pnpm --filter @esl-pipeline/md-extractor build",
    "build:importer": "pnpm --filter @esl-pipeline/notion-importer build",
    "build:orchestrator": "pnpm --filter @esl-pipeline/orchestrator build",
    "build:tts": "pnpm --filter @esl-pipeline/tts-elevenlabs build",
    "build:uploader": "pnpm --filter @esl-pipeline/storage-uploader build",
    "build:validator": "pnpm --filter @esl-pipeline/md-validator build",
    "changeset": "changeset",
    "clean": "pnpm -r --filter './packages/*' run clean",
    "cli:add-audio": "node packages/notion-add-audio/dist/bin/cli.js",
    "cli:colorizer": "node packages/notion-colorizer/dist/bin/cli.js",
    "cli:importer": "node packages/notion-importer/dist/bin/cli.js",
    "cli:orchestrator": "node packages/orchestrator/dist/cli.js",
    "cli:tts": "node packages/tts-elevenlabs/dist/bin/cli.js",
    "cli:uploader": "node packages/storage-uploader/dist/bin/cli.js",
    "dev:add-audio": "pnpm --filter @esl-pipeline/notion-add-audio dev",
    "dev:colorizer": "pnpm --filter @esl-pipeline/notion-colorizer dev",
    "dev:extractor": "pnpm --filter @esl-pipeline/md-extractor dev",
    "dev:importer": "pnpm --filter @esl-pipeline/notion-importer dev",
    "dev:orchestrator": "pnpm --filter @esl-pipeline/orchestrator dev",
    "dev:tts": "pnpm --filter @esl-pipeline/tts-elevenlabs dev",
    "dev:uploader": "pnpm --filter @esl-pipeline/storage-uploader dev",
    "esl": "node packages/orchestrator/dist/cli.js",
    "format": "pnpm exec prettier --check \"**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "format:write": "pnpm exec prettier --write \"**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "lint": "pnpm exec eslint packages",
    "md-extractor": "node packages/md-extractor/dist/bin/cli.js",
    "md-validator": "node packages/md-validator/dist/src/index.js",
    "notion-importer": "node packages/notion-importer/dist/bin/cli.js",
    "release:prepare": "pnpm changeset version && pnpm install",
    "smoke": "pnpm --filter @esl-pipeline/orchestrator test",
    "test": "pnpm -r --filter './packages/*' run test",
    "test:watch": "pnpm -r --filter './packages/*' run test:watch",
    "typecheck": "pnpm -r --filter './packages/*' --if-present run typecheck"
  },
  "dependencies": {
    "@types/mdast": "^4.0.4",
    "remark-parse": "^11.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/eslint-plugin": "^1.5.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "engines": {
    "node": ">=24.11.1"
  }
}